{
 "bindings": [
   {
     "type": "eventHubTrigger",
     "name": "eventHubMessages",
     "direction": "in",
     "path": "insights_metrics_pt1m_name",
     "connection": "AzureEventHubConnectionString",
     "cardinality": "many",
     "consumerGroup": "$Default"
   },
   {
     "type": "blob",
     "name": "outputBlob",
     "path": "sumomet-failover/{rand-guid}",
     "connection": "StorageConnectionString",
     "direction": "out"
   }
 ],
 "disabled": false
}
